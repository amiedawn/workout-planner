{{!-- plans --}}
{{#if classes.length}}
<ul class="collection">
    {{#each classes as |class|}}
    <li class="collection-item avatar">
        <i class="material-icons circle">event</i>
        <span>
          <p>
              <label>
                <input type="checkbox" />
                <span class = "meta">Class Name:{{class_name}} </span>
                </label>
                </p>
        </span>
     </li>
     {{/each}}
    </ul>


{{else}}
<br>
<!--if no plans yet, tell user-->
<h3>No current classes in this category</h3>
{{/if}}

<script src = "/toning.js"></script>